<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxplayer.FXMLDocumentController">
    <children>
        <fx:define>
            <String fx:id="btnText" fx:value="Play" />
        </fx:define>
        <Button text="$btnText" prefWidth="200" prefHeight="60" layoutX="50" layoutY="50" onAction="#handleButtonAction" fx:id="button" />
    </children>
</AnchorPane>
